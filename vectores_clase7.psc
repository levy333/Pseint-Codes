Algoritmo vectores_clase7
	Dimension datos(5);
	i<-0;
	
	//proceso para captura de datos y almacenamiento en el vector
	Para i<-1 Hasta 5 Con Paso  1 Hacer
		Escribir "Digite un valor para la posicion " , i;
		Leer datos(i);
	FinPara
	
	//Proceso para mostrar e imprimir los datos del vector
	
	Para j<-5 hasta 1 con paso -1 Hacer 
		Escribir "El valor guardado en la posicion: " ,j, " es ", datos(j)
	FinPara
	
	
	
FinAlgoritmo

	
